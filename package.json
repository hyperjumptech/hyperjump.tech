{
  "name": "hyperjump.tech",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "generate-locales": "simple-i18n-next -i ./locales -l en && bun scripts/fix-locales-types.js",
    "generate-redirect": "bun .github/scripts/generate-redirect.js",
    "generate-sitemap": "bun .github/scripts/generate-sitemap.js",
    "dev": "bun run generate-locales && TURBOPACK=0 next dev",
    "build": "bun run generate-locales && TURBOPACK=0 next build",
    "postbuild": "bun run generate-sitemap && bun run generate-redirect",
    "start": "bunx serve out",
    "lint": "next lint",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "test:e2e": "playwright test -c playwright.config.ts",
    "test:e2e:headed": "playwright test --headed -c playwright.config.ts",
    "test:e2e:ui": "playwright test --ui -c playwright.config.ts",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@mdx-js/loader": "3.1.1",
    "@mdx-js/react": "3.1.1",
    "@n8n/chat": "0.61.0",
    "@next/mdx": "15.5.6",
    "@next/third-parties": "15.5.6",
    "@radix-ui/react-accordion": "1.2.12",
    "@radix-ui/react-aspect-ratio": "1.1.7",
    "@radix-ui/react-avatar": "1.1.10",
    "@radix-ui/react-context-menu": "2.2.16",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-navigation-menu": "1.2.14",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-toast": "1.2.15",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "framer-motion": "12.23.24",
    "lucide-react": "0.546.0",
    "marked": "16.4.1",
    "next": "16.0.10",
    "next-themes": "0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rehype-stringify": "10.0.1",
    "remark-gfm": "4.0.1",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.2",
    "schema-dts": "1.1.5",
    "simple-i18n-next": "0.0.38",
    "sonner": "2.0.7",
    "tailwindcss-animate": "1.0.7",
    "unified": "11.0.5",
    "unist-util-visit": "5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@tailwindcss/postcss": "^4.1.5",
    "@tailwindcss/typography": "^0.5.16",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "eslint": "^8",
    "eslint-config-next": "^15.5.4",
    "playwright": "^1.56.1",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5"
  }
}
